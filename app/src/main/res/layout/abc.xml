    <me.zhanghai.android.materialratingbar.MaterialRatingBar
        android:id="@+id/library_tinted_normal_ratingbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        android:rating="2.5"
        android:minHeight="48dp"
        android:maxHeight="48dp"
        app:mrb_progressTint="@color/app_color"
        app:mrb_secondaryProgressTint="@color/app_color"
        style="@style/Widget.MaterialRatingBar.RatingBar" />

    <me.zhanghai.android.materialratingbar.MaterialRatingBar
        android:id="@+id/library_normal_ratingbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        android:stepSize="0.1"
        android:visibility="visible"
        android:rating="3.5"
        android:minHeight="@dimen/_50sdp"
        android:maxHeight="@dimen/_50sdp"
        android:layout_marginTop="@dimen/_100sdp"
        style="@style/Widget.MaterialRatingBar.RatingBar"
        />


    <com.google.android.material.button.MaterialButton
        android:id="@+id/btLogin"
        style="@style/PrimaryButtonBlue"
        android:layout_width="@dimen/_150sdp"
        android:layout_height="@dimen/_40sdp"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:paddingHorizontal="0dp"
        android:text="@string/confirm"
        app:iconTint="@color/black"
        android:textColor="@color/black"
        android:backgroundTint="@color/_C08D54"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_below="@+id/library_normal_ratingbar"
        android:layout_marginTop="@dimen/_25sdp"
        app:cornerRadius="@dimen/_7sdp"
        android:visibility="visible"
        app:strokeColor="@color/app_color"
        android:foreground="?android:attr/selectableItemBackground"

        />


