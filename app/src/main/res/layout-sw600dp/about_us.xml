                    android:textSize="@dimen/_9sdp"
                    app:fontFamily="@font/nunitosans_regular"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="HardcodedText"
